{
  "name": "src",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3001",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --fix --ext .js,.ts,.vue ./src --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|vue)\"",
    "stylelint": "stylelint --fix \"**/*.{vue,css}\" --ignore-path ../.gitignore",
    "format": "npm run lint ; npm run prettier ; npm run stylelint"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "keycloak-js": "^21.0.1",
    "vue": "^3.2.45",
    "vue-router": "^4.0.3",
    "vuetify": "^3.0.6",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "postcss": "^8.4.16",
    "postcss-html": "^1.3.0",
    "prettier": "^2.8.2",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.11"
  }
}
